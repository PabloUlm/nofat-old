{
  "name": "node-mongo-nofat-api",
  "version": "1.0.0",
  "description": "NodeJS + MongoDB API for NoFat Management",
  "license": "MIT",
  "scripts": {
    "debug": "node --nolazy --inspect-brk=9229 ./server.js",
    "start": "nodemon ./server.js",
    "start-backend": "nodemon ./index.js",
    "predeploy": "rsync --progress -r . nofat:/app/api --exclude node_modules",
    "deploy": "ssh nofat 'cd /app/api && npm install && service nofat-api restart'"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-fileupload": "^1.1.1-alpha.1",
    "express-jwt": "^5.3.1",
    "json2csv": "^4.3.2",
    "jsonwebtoken": "^8.2.2",
    "mongodb": "^3.0.10",
    "mongoose": "^5.1.4",
    "rootpath": "^0.1.2"
  },
  "devDependencies": {
    "nodemon": "^1.17.5"
  }
}
